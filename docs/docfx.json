{
  "build": {
    "content": [
      {
        "files": [
          "Districts/**.md",
          "Districts/toc.yml",
          "Festivals/**.md",
          "Festivals/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Images/**",
          "CNAME"
        ]
      }
    ],
    "markdownEngineName": "markdig",
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "../material"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appName": "About Bihar",
      "_appTitle": "About Bihar",
      "_appFaviconPath": "Images/favicon.ico",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/bipulRaman/AboutBihar"
      },
      "_gitUrlPattern": "github"
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}