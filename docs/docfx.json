{
  "build": {
    "content": [
      {
        "files": [
          "**/**.md",
          "**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Assets/**",
          "**/Images/**",
          "CNAME"
        ]
      }
    ],
    "markdownEngineName": "markdig",
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "../material"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appName": "About Bihar",
      "_appTitle": "About Bihar",
      "_appFaviconPath": "Assets/favicon.ico",
      "_metaImage":"Assets/default.png",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/bipulRaman/AboutBihar"
      },
      "_gitUrlPattern": "github"
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap": {
      "baseUrl": "https://www.aboutbihar.com/",
      "priority": 0.1
    }
  }
}